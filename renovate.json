{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "schedule": [
    "after 10am every weekday",
    "before 5pm every weekday"
  ],
  "timezone": "Asia/Seoul",
  "branchPrefix": "fixes/dependency/",
  "rangeStrategy": "bump",
  "lockFileMaintenance": {
    "enabled": true
  }
}
